<p id="notice"><%= notice %></p>

<h1>Listing Problems</h1>

<table>
  <thead>
    <tr>
      <th>문제 번호</th>
      <th>제목</th>
      <th>제출</th>
      <th>정답</th>
      <th>정답 비율</th>
    </tr>
  </thead>

  <tbody>
    <% @problems.each do |problem| %>
      <tr>
        <td><%= problem.code %></td>
        <td><%= problem.title %></td>
        <td><%= problem.trial %></td>
        <td><%= problem.success %></td>
        <td><%= problem.percentage / 1000.0 %> %</td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
